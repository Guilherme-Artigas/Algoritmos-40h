algoritmo "exercicio-42 contando5"
var
n, c: inteiro
inicio

      escreva("Digite um numero inteiro e positivo: ")
      leia(n)
      
      c <- 1
      escreva("contagem:")
      enquanto (c <= n) faca
         escreva(c)
         c <- c + 1
      fimenquanto
      escreva(" Acabou!")

fimalgoritmo
