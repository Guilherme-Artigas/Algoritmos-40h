algoritmo "exercicio-77 nome aos contra com vetor"
var
c:inteiro
nome:vetor[1..7] de caractere
inicio

      para c <- 1 ate 7 passo 1 faca
         escreva("Digite seu nome: ")
         leia(nome[c])
         limpatela
      fimpara
      
      para c <- 7 ate 1 passo -1 faca
         escreval(nome[c])
      fimpara

fimalgoritmo
