Algoritmo "somando números inteiros"
Var

   n1, n2, soma: inteiro

Inicio

   escreval("Olá seja bem-vindo!")
   escreval
   escreval("vamos somar dois números inteiros!")
   escreva("Digite o primeiro número: ")
   leia(n1)
   escreva("Digite o segundo número: ")
   leia(n2)
   soma <- n1+n2
   escreval
   escreval("obrigado por me testar! o resultado da soma é ", soma)

Fimalgoritmo
