algoritmo "Teste Vetor"
var
c, soma: inteiro
v:vetor[1..6]de inteiro
inicio

      para c <- 1 ate 6 passo 1 faca
         escreva("Digite o",c,"º valor: ")
         leia(v[c])
         //soma <- soma + v[c]
      fimpara

      escreval

      para c <- 1 ate 6 passo 1 faca
         escreva("{",v[c]:1,"}")
      fimpara

fimalgoritmo
