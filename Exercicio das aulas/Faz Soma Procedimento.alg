algoritmo "Faz Soma Procedimento"
var

   N1, N2: inteiro
   
   procedimento soma(a, b:inteiro)
   inicio
   
      escreval("Recebi o valor", a)
      escreval("Recebi o valor", b)
      escreval("A soma entre os valores",a," e o valor",b," é:",a+b)

   fimprocedimento
   
inicio

      N1 <- 5
      N2 <- 3
      soma(N1, N2)

fimalgoritmo
