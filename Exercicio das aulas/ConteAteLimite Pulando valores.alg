algoritmo "ConteAteLimite Pulando valores"
var
   limite, pulo, contador: inteiro
inicio

      escreva("Digite o limite da contagem: ")
      leia(limite)
      escreva("Pulando de quanto em quantos: ")
      leia(pulo)
      
      enquanto (contador <= limite) faca
         escreval(contador)
         contador <- contador + pulo
      fimenquanto

fimalgoritmo
